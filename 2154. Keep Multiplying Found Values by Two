class Solution {
    public int findFinalValue(int[] nums, int original) {
        // Use a HashSet for fast lookup
        Set<Integer> set = new HashSet<>();
        for (int num : nums) {
            set.add(num);
        }

        // Keep doubling as long as original is found
        while (set.contains(original)) {
            original *= 2;
        }

        return original;
    }
}
